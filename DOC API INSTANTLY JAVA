POST
/api/v2/campaigns

JavaScript



const resp = await fetch(
  `https://api.instantly.ai/api/v2/campaigns`,
  {
    method: 'POST',
    headers: {
      'Content-Type': 'application/json',
      Authorization: 'Bearer <YOUR_TOKEN_HERE>'
    },
    body: JSON.stringify({
      name: 'My First Campaign',
      campaign_schedule: {
        schedules: [
          {
            name: 'My Schedule',
            timing: {
              from: '09:00',
              to: '17:00'
            },
            days: {},
            timezone: 'Etc/GMT+12'
          }
        ]
      }
    })
  }
);

const data = await resp.json();
console.log(data);

