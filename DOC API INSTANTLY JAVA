{
    "campaign_name": "My Test Campaign",
    "campaign_id": "0195f142-174b-7e6d-85eb-5dd6ac41e610",
    "campaign_status": 1,
    "campaign_is_evergreen": true,
    "leads_count": 1500,
    "contacted_count": 1200,
