end_date
string
End date
Example:
end_date=2024-01-01
GET
/api/v2/campaigns/analytics/steps

JavaScript



const query = new URLSearchParams({
  campaign_id: '0195f142-174d-79f4-abea-6ddfaee35eeb',
  start_date: '2024-01-01',
  end_date: '2024-01-01'
}).toString();

const resp = await fetch(
  `https://api.instantly.ai/api/v2/campaigns/analytics/steps?${query}`,
  {
    method: 'GET',
    headers: {
      Authorization: 'Bearer <YOUR_TOKEN_HERE>'
    }
  }
);

const data = await resp.text();
console.log(data);

