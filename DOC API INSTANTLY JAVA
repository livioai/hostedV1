const resp = await fetch(
  `https://api.instantly.ai/api/v2/custom-tags`,
  {
    method: 'POST',
    headers: {
      'Content-Type': 'application/json',
      Authorization: 'Bearer <YOUR_TOKEN_HERE>'
    },
    body: JSON.stringify({label: 'Important'})
  }
);

const data = await resp.json();
